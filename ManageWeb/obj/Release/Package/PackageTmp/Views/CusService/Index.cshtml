@model ManageDomain.Models.PageModel<ManageDomain.Models.CusService>
@{
    ViewBag.Title = "客户服务记录";
    Layout = "~/Views/Shared/_LayoutPageHome.cshtml";
}
@section Head{
    <script type="text/javascript">
        ///<reference path="/scripts/jquery-1.9.1.js" />
        ///<reference path="/scripts/main.js" />
    </script>
}
<h2>客户服务记录<a href="/cusservice/edit" class="btn btn-primary">新增服务记录</a></h2>
<div class="cc-content-header">
    <form class="form-inline" role="form">
        <div class="form-group">
            <label for="search_cusid">客户ID</label>
            <input type="text" class="form-control" id="search_cusid" value="@ViewBag.cusid" name="cusid" placeholder="" />
            <button type="button" class="btn btn-sm" onclick="libcusservice.index_selectcus()">...</button>
        </div>
        <div class="form-group">
            <label for="search_keywords">关键字</label>
            <input type="text" class="form-control" id="search_keywords" value="@ViewBag.keywords" name="keywords" placeholder="搜索关键字" />
        </div>
        <button type="submit" class="btn btn-default">查询</button>
    </form>
</div>
@Html.Partial("Index_Sub", Model)

