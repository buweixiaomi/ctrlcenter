@model ManageDomain.Models.PageModel<ManageDomain.Models.WorkDaily>
@{
    ViewBag.Title = "工作日志列表";
    Layout = "~/Views/Shared/_LayoutPageHome.cshtml";
}
@section Head{
    <script type="text/javascript">
        ///<reference path="/scripts/jquery-1.9.1.js" />
        ///<reference path="/scripts/main.js" />
    </script>
}
<h2>工作日志<a href="/workdaily/add" class="btn btn-primary">新增</a></h2>
<div class="cc-content-header">
    <form class="form-inline" role="form">
        <div class="form-group">
            <label for="search_managerid">人员</label>
            <input type="text" class="form-control" id="search_managerid" value="@ViewBag.search_managerid" name="search_managerid" placeholder="" />
            <button type="button" class="btn btn-sm" onclick="libworkdaily.index_selectmanager()">...</button>
        </div>
        <div class="form-group">
            <label for="search_begintime">工作日期</label>
            <input type="text" class="form-control" id="search_begintime" value="@ViewBag.search_begintime" name="search_begintime" onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd' })" /> 
            <input type="text" class="form-control" id="search_endtime" value="@ViewBag.search_endtime" name="search_endtime" onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd' })" />  
        </div>
        <button type="submit" class="btn btn-default">查询</button>
    </form>
</div>
@Html.Partial("Index_Sub", Model)

