@model ManageDomain.Models.PageModel<ManageDomain.Entity.Summary>
@{
    ViewBag.Title = "summary";
    Layout = "~/Views/Shared/_LayoutPageHome.cshtml";
}

<h2>工作任务汇总</h2>
<div class="cc-content-header">
    <form class="form-inline" role="form">
        <div class="form-group">
            <label for="keywords">用户</label>
            <input type="text" class="form-control" id="distributeuserid" value="@ViewBag.distributeuserid" name="distributeuserid" placeholder="" />
            <button type="button" class="btn btn-sm" onclick="libworkitem.index_selectmanager1()">...</button>
        </div>
        <div class="form-group">
            <label for="ContractBeginTime" class="col-sm-2 control-label">操作时间</label>
            <div class="col-sm-10">
                <input type="text" class="form-control cc-width-2 cc-display-inline-block" id="BeginTime" onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd' })" name="BeginTime" value="@ViewBag.BeginTime">
                <input type="text" class="form-control cc-width-2 cc-display-inline-block" id="EndTime" onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd' })" name="EndTime" value="@ViewBag.EndTime">
            </div>
        </div>
        <button type="submit" class="btn btn-default">查询</button>
    </form>
</div>
@Html.Partial("summary_Sub", Model)