<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>

    <add key="RunInServiceMode" value="false"/>

    <add key="ServerUrl" value="http://101.37.75.51:8070/"/>
    <!--<add key="ServerUrl" value="http://localhost:1890/"/>-->


    <!--批量写日志的批量数-->
    <add key="watchlog:WriteNoBlock" value="true"/>
    <!--批量写日志的批量数-->
    <add key="watchlog:BatchSize" value="1000"/>
    <!--批量写日志时 最大延迟时间-->
    <add key="watchlog:TimeOutSeconds" value="10"/>
    <!--当前的日志类型 数据库日志dbloger或文件日志fileloger，不写日志时请配none-->
    <add key="watchlog:LogerType" value="none"/>
    <!--当前日志的项目名称-->
    <add key="watchlog:ProjectName" value="服务OE"/>
    <!--是否打开耗时日志 默认为true-->
    <add key="watchlog:OpenTimeWatch" value="true"/>
    <!--文件日志保存的路经 默认为当前程序目录下的WatchLog文件夹-->
    <add key="watchlog:FileLogPath" value="~\WatchLog"/>
    <!--文件日志是否合并到一个文件 默认为否-->
    <add key="watchlog:FileUnion" value="false"/>
    <!--数据库日志的连接字符串-->
    <add key="watchlog:DbConnString" value="server=xx;port=xx;database=xx;user id=xx;password=xx;CharSet=utf8;Allow Zero Datetime=true"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>


















